# ---- Functions ----

def transcribe_audio(file_path):
    model = whisper.load_model("base")  # or "small", "medium", "large"
    result = model.transcribe(file_path)
    return result["text"]

def query_ollama(prompt: str, transcript: str):
    payload = {
        "model": OLLAMA_MODEL,
        "prompt": f"{prompt.strip()}\n\n{transcript.strip()}",
        "stream": False
    }
    response = requests.post(OLLAMA_URL, json=payload)
    response.raise_for_status()
    return response.json()["response"]

def save_json(obj, path):
    with open(path, "w", encoding="utf-8") as f:
        json.dump(obj, f, ensure_ascii=False, indent=2)

# ---- Main ----

def main(audio_file):
    print(f"[+] Transcribing {audio_file} ...")
    transcript = transcribe_audio(audio_file)
    Path("output").mkdir(exist_ok=True)
    with open("output/transcript.txt", "w") as f:
        f.write(transcript)

    print("[+] Querying for qualification JSON ...")
    result1 = query_ollama(PROMPT_QUALIFICATION, transcript)
    save_json(json.loads(result1), "output/summary.json")

    print("[+] Querying for critical issues ...")
    result2 = query_ollama(PROMPT_CRITICAL_ISSUES, transcript)
    save_json(json.loads(result2), "output/issues.json")

    print("[+] Querying for categorization ...")
    result3 = query_ollama(PROMPT_TYPING, transcript)
    save_json(json.loads(result3), "output/typology.json")

    print("[✓] All outputs saved in /output/")

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: python process_audio.py path/to/audio.wav")
        sys.exit(1)
    main(sys.argv[1])
